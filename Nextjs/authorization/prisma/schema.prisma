datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
  engineType      = "client"
}

model Users {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  username  String?
  name      String?
  surname   String?
  createdAt DateTime @default(now())
}